{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": ["node_modules"],
  "include": [
    ".plasmo/index.d.ts",
    "next-env.d.ts",
    "./env.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "paths": {
      "~*": ["./*"] //,
    },
    "baseUrl": ".",
    "strict": true,
    "verbatimModuleSyntax": false,
    "esModuleInterop": true,
    "moduleResolution": "Bundler"
  }
}
